public class Motor {
    private int cilindrada;
    private double velocidadMax;
    private int caballosFuerza;
    private String tipoCombustible;

    // Constructor
    public Motor(int cilindrada, double velocidadMax, int caballosFuerza, String tipoCombustible) {
        this.cilindrada = cilindrada;
        this.velocidadMax = velocidadMax;
        this.caballosFuerza = caballosFuerza;
        this.tipoCombustible = tipoCombustible;
    }

    // Método imprimir
    public void imprimir() {
        System.out.println("Cilindrada: " + cilindrada + ", VelocidadMax: " + velocidadMax 
                           + ", CaballosFuerza: " + caballosFuerza + ", TipoCombustible: " + tipoCombustible);
    }

    // Getters y Setters
    public int getCilindrada() {
        return cilindrada;
    }

    public void setCilindrada(int cilindrada) {
        this.cilindrada = cilindrada;
    }

    public double getVelocidadMax() {
        return velocidadMax;
    }

    public void setVelocidadMax(double velocidadMax) {
        this.velocidadMax = velocidadMax;
    }

    public int getCaballosFuerza() {
        return caballosFuerza;
    }

    public void setCaballosFuerza(int caballosFuerza) {
        this.caballosFuerza = caballosFuerza;
    }

    public String getTipoCombustible() {
        return tipoCombustible;
    }

    public void setTipoCombustible(String tipoCombustible) {
        this.tipoCombustible = tipoCombustible;
    }
}

public class Vehiculo {
    private String placa;
    private String marca;
    private Motor motor;
    private int cilindrada;
    private double velocidadMax;
    private int caballosFuerza;
    private String tipoCombustible;

    // Constructor que recibe placa y marca
    public Vehiculo(String placa, String marca) {
        this.placa = placa;
        this.marca = marca;
        this.motor = new Motor(1400, 240, 4, "Diesel"); // Valores por defecto
    }

    // Constructor que recibe todos los parámetros
    public Vehiculo(String placa, String marca, int cilindrada, double velocidadMax, int caballosFuerza, String tipoCombustible) {
        this.placa = placa;
        this.marca = marca;
        this.motor = new Motor(cilindrada, velocidadMax, caballosFuerza, tipoCombustible);
    }

    // Método imprimir
    public void imprimir() {
        System.out.println("Placa: " + placa + ", Marca: " + marca);
        motor.imprimir();
    }

    // Getters y Setters
    public String getPlaca() {
        return placa;
    }

    public void setPlaca(String placa) {
        this.placa = placa;
    }

    public String getMarca() {
        return marca;
    }

    public void setMarca(String marca) {
        this.marca = marca;
    }

    public int getCilindrada() {
        return motor.getCilindrada();
    }

    public void setCilindrada(int cilindrada) {
        motor.setCilindrada(cilindrada);
    }

    public double getVelocidadMax() {
        return motor.getVelocidadMax();
    }

    public void setVelocidadMax(double velocidadMax) {
        motor.setVelocidadMax(velocidadMax);
    }

    public int getCaballosFuerza() {
        return motor.getCaballosFuerza();
    }

    public void setCaballosFuerza(int caballosFuerza) {
        motor.setCaballosFuerza(caballosFuerza);
    }

    public String getTipoCombustible() {
        return motor.getTipoCombustible();
    }

    public void setTipoCombustible(String tipoCombustible) {
        motor.setTipoCombustible(tipoCombustible);
    }
}
public class ComparadorVehiculos {
    public static void compararMotores(Vehiculo v1, Vehiculo v2) {
        if (v1.getCilindrada() == v2.getCilindrada()) {
            System.out.println("Los vehículos tienen la misma cilindrada.");
        } else if (v1.getCilindrada() > v2.getCilindrada()) {
            System.out.println("El vehículo con placa " + v1.getPlaca() + " tiene mayor cilindrada.");
        } else {
            System.out.println("El vehículo con placa " + v2.getPlaca() + " tiene mayor cilindrada.");
        }
    }
}
public class Main {
    public static void main(String[] args) {
        // Crear dos objetos de tipo Vehiculo
        Vehiculo vehiculo1 = new Vehiculo("ABC123", "Toyota", 1600, 220, 6, "Gasolina");
        Vehiculo vehiculo2 = new Vehiculo("XYZ789", "Honda", 1400, 210, 4, "Diesel");

        // Imprimir características de los vehículos
        vehiculo1.imprimir();
        vehiculo2.imprimir();

        // Comparar cilindradas de los motores
        ComparadorVehiculos.compararMotores(vehiculo1, vehiculo2);
    }
}
